<script>\n\nimport axios from 'axios'\nimport Pokemon from './components/Pokemon'\n\n\nexport default {\n  components: { Pokemon },\n  name: 'App',\n  data(){\n    return{\n      pokemons : [],\n      filterPokemons: [],\n      busca: ''\n    }\n  },\n  created: function(){\n    axios.get(\"https://pokeapi.co/api/v2/pokemon?limit=151&offset=0\").then((res)=>{\n\n      this.pokemons= res.data.results\n      this.filterPokemons = res.data.results\n    })\n  },\n  component: {\n   Pokemon\n },\n methods:{\n   buscar: function(){\n     this.filterPokemons = this.pokemons\n     if(this.busca == '' || this.busca ==' '){\n        this.filterPokemons == this.pokemons\n     }else{\n       this.filterPokemons = this.pokemons.filter(pokemons => pokemons.name == this.busca)\n     }\n   }\n },\n computed:{\n   /*\n   resultadoBusca: function(){\n     if(this.busca == '' || this.busca ==' '){\n       return this.pokemons\n     }else{\n       return this.pokemons.filter(pokemons => pokemons.name == this.busca)\n     }\n   }*/\n }\n}\n\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n \n}\n#img_poke {\n  margin-top: 1%;\n  width: 20%;\n  \n}\n#buscaBtn{\n  margin-top: 2%;\n}\n</style>\n"]}]}